{
  "expo": {
    "name": "Project BEAM",
    "slug": "project-beam",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/my-new-logo.png",
    "scheme": "kuryentipid",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "package": "com.duxxous.kuryenttipidcalculator"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "react-native-reanimated/plugin",
        {
          "relativeSourceLocation": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5f168bad-56c3-47d0-9ce9-2f5d4657bb38"
      }
    }
  }
}
